<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="wrapper" prefHeight="95.0" prefWidth="203.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aservio.management.Management">
   <children>
      <BorderPane fx:id="borderPane" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <HBox spacing="10.0">
                     <children>
                        <HBox fx:id="adminToolbar" spacing="10.0">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </HBox>
                        <Pane HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Pane>
                        <HBox fx:id="viewToolbar" alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" spacing="5.0">
                           <children>
                              <SplitMenuButton fx:id="viewMenu" mnemonicParsing="false" text="Views">
                                <items>
                                  <MenuItem mnemonicParsing="false" onAction="#handleShowMonth" text="Month" />
                                  <MenuItem mnemonicParsing="false" onAction="#handleShowWeek" text="Week" />
                                    <MenuItem mnemonicParsing="false" onAction="#handleShowDay" text="Day" />
                                </items>
                              </SplitMenuButton>
                            <Button mnemonicParsing="false" onAction="#handlePrevious" text="&lt;" />
                              <Button mnemonicParsing="false" onAction="#handleNext" text="&gt;" />
                           </children>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
      <HBox alignment="BOTTOM_LEFT" maxHeight="40.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="leftStatus" text="Left status" />
            <Pane maxHeight="30.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="rightStatus" text="Right status" />
            <ProgressIndicator fx:id="progressIndicator" progress="0.0" translateY="20.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ProgressIndicator>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
