<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="200.0" minWidth="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aservio.management.overview.OverviewActivityPopup">
   <children>
      <HBox alignment="TOP_RIGHT" prefWidth="290.0" spacing="10.0">
         <children>
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#buttonAction" text="Rediger" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#buttonAction" text="X" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <Label fx:id="titleLabel" text="Overskrift" />
      <Label fx:id="dateInformationLabel" text="tidsinformationer" />
      <Label fx:id="moreInformationLabel" text="flere informationer" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
