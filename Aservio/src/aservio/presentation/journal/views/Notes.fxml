<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aservio.presentation.journal.controllers.overview.Notes">
   <children>
      <VBox alignment="TOP_CENTER" spacing="20.0">
         <children>
            <Pane />
            <Label fx:id="dateLabel" text="Dato" />
            <Label fx:id="startTimeLabel" text="Start tidspunkt" />
            <Label fx:id="endTimeLabel" text="Slut tidspunkt" />
            <Pane VBox.vgrow="ALWAYS" />
            <Button fx:id="modifyButton" mnemonicParsing="false" text="Rediger" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets bottom="5.0" />
         </HBox.margin>
         <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox alignment="TOP_CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="titelLabel" text="Citizen +Titel" />
            <TextArea fx:id="noteTextArea" editable="false" VBox.vgrow="ALWAYS" />
            <Label fx:id="footNoteLabel" text="Personale +Sted" />
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <padding>
            <Insets right="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
