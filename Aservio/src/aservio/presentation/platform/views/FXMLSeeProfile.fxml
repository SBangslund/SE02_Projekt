<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aservio.presentation.platform.controllers.FXMLSeeProfileController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <HBox>
               <children>
                  <VBox minWidth="125.0" spacing="10.0">
                     <children>
                        <HBox>
                           <children>
                              <Label text="Fornavn" />
                              <Pane HBox.hgrow="ALWAYS" />
                              <Label fx:id="labelFirstName" text="labelFirstName" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Label text="Efternavn" />
                              <Pane HBox.hgrow="ALWAYS" />
                              <Label fx:id="labelLastName" text="labelLastName" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Pane HBox.hgrow="ALWAYS" />
                  <ImageView fx:id="imagePerson" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox>
               <children>
                  <Label text="Adresse:" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelAddress" text="labelAddress">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="By:" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelCity" text="labelCity">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="Postnummer:" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelPostcode" text="labelPostcode">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox>
               <children>
                  <Label text="Telefon nr:" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelPhoneNumber" text="labelPhoneNumber">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="Mail:" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelMail" text="labelMail">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="Institution" />
                  <Pane minWidth="50.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="labelInstitution" text="labelInstitution">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
